set imap_delim_chars="/" # necessary to access mailboxes with dots in them..
# https://bugs.debian.org/cgi-bin/bugreport.cgi?bug=534912

set my_email_pass="PASSWORD"
set from="FROM_ADDRESS"
set use_from=yes
set envelope_from=yes

set imap_user="USER"
set imap_pass=$my_email_pass

set smtp_url=SMTP_URL
set smtp_pass=$my_email_pass

set folder="imaps://imap.gmail.com:993"
set spoolfile="+INBOX"
set ssl_starttls = yes
